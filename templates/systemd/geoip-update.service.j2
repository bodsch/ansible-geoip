[Unit]
Description         = update geoip database.
After               = syslog.target network.target

[Service]
Type                = oneshot

WorkingDirectory    = /tmp
User                = root
Group               = root

ExecStart           = /bin/geoip_update.py

SyslogIdentifier    = geoip-update

[Install]
WantedBy            = multi-user.target
